{
    "languageserver": {
        "hdlChecker": {
            "command": "hdl_checker",
            "args": [
                "--lsp"
            ],
            "filetypes": [
                "vhdl",
                "verilog",
                "systemverilog"
            ]
        },
        "godot":{
            "host": "${env:WINLOCALHOST}",
            "port": 6005,
            "filetypes": ["gdscript"]
        },
        "csharp-ls":{
            "command": "csharp-ls.exe",
            "filetypes": ["cs"],
            "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"],
            "enable": false
        }
    },

    "cSpellExt.enableDictionaries": ["spanish"],
    "cSpell.language": "es,en",
    "cSpell.minWordLength": 3,
    "cSpell.enabledLanguageIds": [
        "javascript",
        "javascriptreact",
        "markdown",
        "vimwiki",
        "plaintext",
        "typescript",
        "typescriptreact",
        "yml"
    ],
